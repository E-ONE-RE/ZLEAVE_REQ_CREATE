{
	"_version": "1.5.0",
	"sap.app": {
		"id": "ZLEAVE_REQ_CREATE",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		},
		"dataSources": {
			"ZLEAVE_REQUEST_SRV": {
				"uri": "/sap/opu/odata/sap/ZLEAVE_REQUEST_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/ZLEAVE_REQUEST_SRV/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_bluecrystal"]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "ZLEAVE_REQ_CREATE.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.collaboration": {},
				"sap.m": {},
				"sap.me": {
					"minVersion": ""
				},
				"sap.ui.comp": {},
				"sap.ui.core": {},
				"sap.ui.layout": {},
				"sap.ui.unified": {
					"minVersion": ""
				},
				"sap.ushell": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "ZLEAVE_REQ_CREATE.i18n.i18n"
				}
			},
			"": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "ZLEAVE_REQUEST_SRV",
				"preload": true
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"targetClass": "sap.m.routing.Targets",
				"viewType": "XML",
				"viewPath": "ZLEAVE_REQ_CREATE.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"targetParent": "Page",
				"transition": "slide",
				"clearAggregation": false,
				"bypassed": {
					"target": "notFound"
				}
			},
			"routes": [{
				"name": "view1",
				"pattern": "",
				"greedy": false,
				"target": "view1",
				"view": "View1"
			}, {
			
				"name": "view1s",
				"pattern": "view1s",
				"greedy": false,
				"target": "view1s",
				"view": "View1s"
			}, {
				"name": "view2",
				"pattern": "view2/{ZrequestId}",
				"greedy": false,
				"target": "view2",
				"view": "View2"
			}],
			"targets": {
				"view1": {
					"viewName": "View1",
					"viewId": "V1",
					"viewLevel": 1
				},
				"view2": {
					"viewName": "View2",
					"viewId": "V2",
					"viewLevel": 3,
					"transition": "flip"
				},
				"notFound": {
					"viewName": "NotFound",
					"transition": "show"
				},
				"view1s": {
					"transition": "slide",
					"viewName": "View1s",
					"viewId": "V1S",
					"viewLevel": 2
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		}
	}
}